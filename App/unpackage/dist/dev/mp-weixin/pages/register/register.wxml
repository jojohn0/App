<view class="register"><view class="reg-title">注册</view><view class="reg-phone reg-public"><input type="text" placeholder="请输入手机号码" data-event-opts="{{[['input',[['accountInput',['$event']]]]]}}" bindinput="__e"/><image class="yes" style="{{(display)}}" src="../../static/image/login/correct.png"></image></view><view class="reg-password reg-public"><input type="{{type1}}" placeholder="请输入密码" data-event-opts="{{[['input',[['passwordInput',['$event']]]]]}}" bindinput="__e"/><image class="look" src="{{lookurl1}}" data-event-opts="{{[['tap',[['looks1',['$event']]]]]}}" bindtap="__e"></image></view><view class="reg-confirm reg-public"><input type="{{type2}}" placeholder="再次输入密码" data-event-opts="{{[['input',[['confirmPassword',['$event']]]]]}}" bindinput="__e"/><image class="look" src="{{lookurl2}}" data-event-opts="{{[['tap',[['looks2',['$event']]]]]}}" bindtap="__e"></image></view><button data-event-opts="{{[['tap',[['register',['$event']]]]]}}" class="{{[[(isok)?'but':''],[(!isok)?'but1':'']]}}" bindtap="__e">注册</button></view>