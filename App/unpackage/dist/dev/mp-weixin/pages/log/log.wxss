.logContainer {
  padding-top: 30rpx;
  height: 100%;
}
/* search start */
.logHeader {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
  margin-bottom: 30rpx;
}
.logHeader input {
  -webkit-border-radius: 15px;
          border-radius: 15px;
  padding-left: 40rpx;
  font-size: 15px;
  width: 85%;
  height: 80rpx;
  background-color: rgb(246, 246, 246);
}
/* search end */
/* folder start */
.folder {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  width: 90%;
  margin-top: 30rpx;
  margin-bottom: 30rpx;
  margin-left: 5%;
  padding-right: 30rpx;
  height: 100rpx;
  -webkit-border-radius: 14px;
          border-radius: 14px;
  background-color: rgb(246, 246, 246);
}
.folder .folder_item {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  margin: 0 20rpx;
}
.folder_icon image {
  width: 20px;
  height: 20px;
}
.folder_name {
  margin-left: 15rpx;
}
/* folder end */
/* log_item start */
.log_item {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: vertical;
  -webkit-box-direction: normal;
  -webkit-flex-direction: column;
          flex-direction: column;
  width: 90%;
  -webkit-border-radius: 15px;
          border-radius: 15px;
  background-color: rgb(246, 246, 246);
  margin: 0 20rpx;
  margin-left: 5%;
  padding-left: 3%;
  padding-top: 20rpx;
  padding-bottom: 20rpx;
  margin-bottom: 30rpx;
}
.log_item > view {
  overflow: hidden;
  -webkit-line-clamp: 1;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  margin: 3rpx 0;
}
.item_name {
  font-size: 17px;
  font-weight: bold;
}
.item_info {
  font-size: 15px;
  color: rgb(113, 113, 113);
}
.item_date {
  font-size: 13px;
  color: rgb(113, 113, 113);
}
/* log_item end */
/* new_fixed start */
.new_fixed {
  position: fixed;
  bottom: 50rpx;
  right: 30rpx;
}
.new_fixed image {
  width: 80rpx;
  height: 80rpx;
}
/* new_fixed end */

